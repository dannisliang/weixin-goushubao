<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 3.1//EN" "http://tuckey.org/res/dtds/urlrewrite3.1.dtd">
<urlrewrite>
    <rule>
        <from>^/([a-zA-Z]*)\/([a-zA-Z]*).html</from>
        <to>/$1/$2.action</to>
    </rule>
    <outbound-rule>
        <from>^/([a-zA-Z]*)\/([a-zA-Z]*).action</from>
        <to>/$1/$2.html</to>
    </outbound-rule>
</urlrewrite>